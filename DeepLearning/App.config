<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="Width" value="35"/> <!-- Width of each training image -->
    <add key="Height" value="35"/> <!-- Height of each training image -->
    <add key="ClassCount" value="2"/> <!-- Number of unique classes -->
    <add key="TrainCount" value="0"/> <!-- Max number of rows to read from the training set (useful for preparing learning curves). Set to 0 for all data. -->
    <add key="EpochCount" value="600"/> <!-- Number of epochs for each neural network layer to train on. -->
    <add key="Sigma" value="12"/> <!-- SVM sigma training value, smoothes the gaussian ie., fuzziness factor (lower values = lower bias and higher variance and maybe overfitting, higher values = higher bias and lower variance). -->
    <add key="TrainPath" value="../../../data/catsdogs-train.csv"/> <!-- Path to training csv file -->
    <add key="CvPath" value="../../../data/catsdogs-cv.csv"/> <!-- Path to CV csv file -->
    <add key="TestPath" value="../../../data/catsdogs_test.csv"/> <!-- Path to test csv file -->
  </appSettings>
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
</configuration>